<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >
    
    <Button 
        android:id="@+id/btn_am"
        android:layout_height="wrap_content"
        android:layout_width="fill_parent"
        android:text="RunningTaskInfos"/>
    <Button 
        android:id="@+id/btn_am1"
        android:layout_height="wrap_content"
        android:layout_width="fill_parent"
        android:text="RecentTaskInfos"/>
    <Button 
        android:id="@+id/btn_am2"
        android:layout_height="wrap_content"
        android:layout_width="fill_parent"
        android:text="getAllTheLauncher"/>
    <Button 
        android:id="@+id/btn_am3"
        android:layout_height="wrap_content"
        android:layout_width="fill_parent"
        android:text="getLauncherRunnig"/>

    <TextView 
        android:id="@+id/tv_am"
        android:layout_height="wrap_content"
        android:layout_width="fill_parent"
        />
</LinearLayout>
